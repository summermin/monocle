<% if current_user %>
<div>Welcome, <%= current_user.first_name %>!</div>
<div><%= link_to 'Check out your projects', projects_path %></div>
<div><%= link_to "Create a new project", new_user_project_path(current_user) %></div>
<% else %>
<div>Sign in <%= link_to "here!", new_user_session_path %></div>
<% end %>